{
    "manifest_version": 3,
    "name": "CryptoFiat",
    "description": "Converts any fiat currencies displayed on a page into your chosen cryptocurrency! (BTC, XMR, ETH, SOL)",
    "version": "1.0",
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/cryptofiat_icon_128.png"
    },
    "content_scripts":
    [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        
        "storage"

    ]
}